<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

 <path>LGHI_BigLight.ac</path>

 <animation>
  <name>Shifting</name>
<object-name>FlashLeader</object-name>
<object-name>Light.000</object-name>
<object-name>Light.001</object-name>
<object-name>Light.002</object-name>
<object-name>Light.003</object-name>
<object-name>Light.004</object-name>
<object-name>Light.005</object-name>
<object-name>Light.006</object-name>
<object-name>Light.007</object-name>
<object-name>Light.008</object-name>
<object-name>Light.009</object-name>
<object-name>LightPole</object-name>
<object-name>Studs</object-name>
<object-name>Studs2</object-name>
 </animation>

 <animation>
  <object-name>Shifting</object-name>
  <type>textranslate</type>
    <property>/sim/time/sun-angle-rad</property>
    <step> 1.57 </step>
    <factor>0.318471338</factor>
  <axis>
   <x>-1</x>
   <y>0</y>
  </axis>
 </animation>

 <animation>
  <name>Highlight</name>
    <enable-hot type="bool">false</enable-hot>
  <object-name>Glass0</object-name>
  <object-name>Glass1</object-name>
  <object-name>Glass2</object-name>
  <object-name>Glass3</object-name>
  <object-name>Glass4</object-name>
  <object-name>Glass5</object-name>
  <object-name>Glass6</object-name>
  <object-name>Glass7</object-name>
  <object-name>Glass8</object-name>
  <object-name>Glass9</object-name>
  <object-name>Cone</object-name>
  <object-name>Cone.001</object-name>
  <object-name>Cone.002</object-name>
  <object-name>Cone.003</object-name>
  <object-name>Cone.004</object-name>
  <object-name>Cone.005</object-name>
  <object-name>Cone.006</object-name>
  <object-name>Cone.007</object-name>
 </animation>

 <animation>
   <type>select</type>
    <enable-hot type="bool">false</enable-hot>
    <object-name>Highlight</object-name> 
   <condition>		
     <greater-than-equals>		
       <property>/sim/time/sun-angle-rad</property>
       <value>1.57</value>
     </greater-than-equals>
   </condition>
 </animation>

  <animation>
    <type>material</type> 
    <object-name>Highlight</object-name> 
 <condition>		
  <greater-than-equals>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than-equals>
 </condition>
 <emission>	
  <red>0.2</red>	
  <green>0.2</green>	
  <blue>0.2</blue>
 </emission>
  </animation>

</PropertyList>

